MODULE CaseTest;

PROCEDURE Start;
BEGIN
  WriteChar(41X)
END Start;

PROCEDURE FancyRange(ch :CHAR);
VAR i :INTEGER;
BEGIN
  i := 1;
  CASE ch OF
    "A" .. "Z", "a" .. "z", "0" .. "9", "_": i := 2;
  END;
  WriteInt(i); WriteLn
END FancyRange;

PROCEDURE Trial(a :INTEGER);
BEGIN
  CASE a OF
    0 :WriteInt(1000)
  | 1 :WriteInt(1001)
  | 2 :WriteInt(1002)
  | 3 :WriteInt(1003)
  | 4 :WriteInt(1004)
  | 5 :WriteInt(1005)
  | 8 .. 10 :WriteInt(3333)
  END;
  WriteLn
END Trial;

BEGIN
  Start;
  Trial(-5);
  Trial(33);
  Trial(0);
  Trial(1);
  Trial(2);
  Trial(3);
  Trial(4);
  Trial(5);
  Trial(6);
  Trial(7);
  Trial(8);
  Trial(10);
  FancyRange("A");
  FancyRange("E");
  FancyRange("Z");
  FancyRange("a");
  FancyRange("e");
  FancyRange("z");
  FancyRange(","); (* Smaller than 0  *)
  FancyRange("{"); (* Larger than z   *)
  FancyRange("^"); (* Between Z and a *)
END CaseTest.
