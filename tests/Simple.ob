MODULE Simple;

VAR
  a, b, c, d, e, f, g :INTEGER;
BEGIN
  IF a > b THEN
    c := 100;
  END;
  IF (c > d) OR (e > f) THEN
    g := 200;
  END;
  IF (f > g) & (g > a) THEN
    b := 300;
  END;
  IF ((a > b) OR (c > d) OR (e > f)) &
     ((a < 10) OR (b < 20) OR (c < 30)) THEN
    c := 400;
  ELSIF ((a > b) & (c > d) & (e > f)) OR
     ((a < 10) & (b < 20) & (c < 30)) THEN
    c := 500;
  ELSE
    c := 600;
  END;
  a := 999;
  a := 1000 
END Simple.
